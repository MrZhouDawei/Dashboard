{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js",
      "methods": ["GET","POST","OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://dashboard-rework.vercel.app"
      }
    }
  ]
}
